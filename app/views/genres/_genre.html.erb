<div class="col-md-3">
	<table class="genre-search">
		<thead>
			<tr>
				<th class="text-center">ジャンル検索</th>
			</tr>
		</thead>
		<tbody>
			<% genres.each do |genre| %>
				<tr>
					<td>
						<%= link_to genre.genre, controller: "pictures", action: "comics", genre_id: genre.id %>
					</td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>